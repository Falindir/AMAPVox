<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="623.0" prefWidth="504.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.amap.lidar.amapvox.gui.PositionImporterFrameController">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="102.0" prefWidth="431.0">
         <children>
            <Label text="Single position">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <GridPane alignment="CENTER" prefHeight="60.0" prefWidth="497.0">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="X" />
                  <Label text="Y" GridPane.columnIndex="1" />
                  <Label text="Z" GridPane.columnIndex="2" />
                  <TextField fx:id="textFieldXPosition" GridPane.rowIndex="1" />
                  <TextField fx:id="textFieldYPosition" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="textFieldZPosition" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <Button mnemonicParsing="false" onAction="#onActionButtonAddSinglePosition" text="Add position" />
         </children>
      </VBox>
      <Button fx:id="onActionButtonImportFromFile" layoutX="14.0" layoutY="329.0" mnemonicParsing="false" onAction="#onActionImportPositionsFromFile" text="Import from file">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <VBox layoutX="14.0" layoutY="141.0" spacing="10.0">
         <children>
            <Label text="Squared area around a point (Grid sampling)">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label text="Point (relative)" />
                  <TextField fx:id="textfieldScannerPosCenterX" prefHeight="25.0" prefWidth="63.0" promptText="X" />
                  <TextField fx:id="textfieldScannerPosCenterY" prefHeight="25.0" prefWidth="63.0" promptText="Y" />
                  <TextField fx:id="textfieldScannerPosCenterZ" prefHeight="25.0" prefWidth="63.0" promptText="Z offset" />
               </children>
            </HBox>
            <HBox spacing="20.0">
               <children>
                  <HBox alignment="CENTER">
                     <children>
                        <Label text="Width" />
                        <TextField fx:id="textfieldScannerWidthArea" prefHeight="25.0" prefWidth="63.0" text="10" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER">
                     <children>
                        <Label text="Step" />
                        <TextField fx:id="textfieldScannerStepArea" prefHeight="25.0" prefWidth="63.0" text="0.5" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Separator layoutX="14.0" layoutY="133.0" prefHeight="3.0" prefWidth="333.0" />
      <Separator layoutX="14.0" layoutY="310.0" prefHeight="3.0" prefWidth="333.0" />
      <Separator layoutX="16.0" layoutY="371.0" prefHeight="3.0" prefWidth="333.0" />
      <HBox alignment="CENTER" layoutX="14.0" layoutY="242.0">
         <children>
            <Label text="Voxel file" />
            <TextField fx:id="textfieldVoxelFile" prefHeight="25.0" prefWidth="229.0" />
            <Button mnemonicParsing="false" onAction="#onActionButtonOpenVoxelFile" text="Open" />
         </children>
      </HBox>
      <VBox layoutX="14.0" layoutY="395.0" prefHeight="174.0" prefWidth="333.0">
         <children>
            <Label text="Positions">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <ListView fx:id="listViewCanopyAnalyzerSensorPositions" prefWidth="315.0" />
            <HBox spacing="10.0">
               <children>
                  <MenuButton mnemonicParsing="false" text="Selection">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#onActionMenuItemPositionsSelectionAll" text="All" />
                        <MenuItem fx:id="menuItemSelectionNone1" mnemonicParsing="false" onAction="#onActionMenuItemPositionsSelectionNone" text="None" />
                     </items>
                  </MenuButton>
                  <HBox spacing="5.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#onActionButtonRemovePosition">
                           <graphic>
                              <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/gnome_list_remove.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Button layoutX="402.0" layoutY="547.0" mnemonicParsing="false" onAction="#onActionButtonImportPositions" text="Import positions" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" />
      <Button fx:id="onActionButtonImportFromFile1" layoutX="14.0" layoutY="277.0" mnemonicParsing="false" onAction="#onActionButtonGenerateGridPosition" text="Generate grid sampling">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
